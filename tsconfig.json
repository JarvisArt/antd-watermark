{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "preserve",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.umi/*"],
      "rc-watermark": ["src/index.tsx"]
    }
  },
  "include": [".dumi/**/*", ".dumirc.ts", "**/*.ts", "**/*.tsx"]
}
